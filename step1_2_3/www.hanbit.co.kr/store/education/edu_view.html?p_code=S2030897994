            var container = document.getElementById('map');
            var options = {
              center: new daum.maps.LatLng(37.559206, 126.927819),
              level: 3
            };
        
            var map = new daum.maps.Map(container, options);
            
            var markerPosition  = new daum.maps.LatLng(37.559206, 126.927819);                       
            
            
            function view_hover_map() {

              var i,p,v,obj,args=view_hover_map.arguments;
              for (i=0; i<(args.length-2); i+=3) 
              with (document) if (getElementById && ((obj=getElementById(args[i]))!=null)) { v=args[i+2];
              if (obj.style) { obj=obj.style; v=(v=='show')?'block':(v=='hide')?'none':v; }
              obj.display=v; }
              
              map.relayout();
              map.setCenter(new daum.maps.LatLng(37.559206, 126.927819));
              
              // 마커를 생성합니다
              var marker = new daum.maps.Marker({
                position: markerPosition
              });
              
              // 마커가 지도 위에 표시되도록 설정합니다
              marker.setMap(map);
              }


          </script>


