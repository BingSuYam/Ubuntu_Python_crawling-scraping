<script>
function hb_show_tabs(t_num){        
  var t_max = 7;
  for(var t=1; t <t_max; t++){    
    if(t_num ==0){
      $("#tabs_"+t).show();
      $(".detail_tit").css({"display":"block"});
    }else{      
      if(t==t_num){ 
        $("#tabs_"+t).show();  
        $(".detail_tit").css({"display":"none"});      
      }else{
        $("#tabs_"+t).hide();    
      }
    }
  }
  
}
hb_show_tabs(0);
</script>
        
