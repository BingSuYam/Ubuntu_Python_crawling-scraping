<!-- old // 구현시 필요없으면 삭제해도 됩니다.
<script type="text/javascript">
  function sch_smit(){
    if(!$.trim($("#keyword_str").val()).length) {
      alert("검색어를 입력해주세요");
      $("#keyword_str").focus();
      return false;
    }else{
      $("#gnbsearchfrm").attr("action","/search/search_list.html");
      $("#gnbsearchfrm").submit(); 
    }
  }
</script>
