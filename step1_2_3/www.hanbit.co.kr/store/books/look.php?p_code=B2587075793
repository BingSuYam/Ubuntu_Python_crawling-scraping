<script type="text/javascript">
    "use strict";

    var common_top_search_vue = new Vue({
        el: '#main_search_area',
        data: {
            //검색엔진 URL
            searchProxyURL: "https://search.hanbit.co.kr",
            //인기 검색어 show 여부
            showPopularKeyword: true,
            //사용자 검색 키워드
            keyword: '',
            //인기 검색어 리스트
            popularKeywordList: [],
            // 인기 검색어 리스트 (왼쪽)
            leftPopularKeywordList: [],
            // 인기 검색어 리스트 (오른쪽)
            rightPopularKeywordList: [],
            // 자동완성에서의 책 리스트
            bookList: [],
            // 자동완성에서의 책 정보
            bookSummary: {
                'imgUrl': '',
                'link': '',
                'name': '',
                'author': '',
                'publishingCompany': '',
                'date': '',
                'bookType': ''
            },
            //에듀케이션 리스트
            educationList: [],
            //컨텐츠 리스트
            contentList: []
        },
        created: function created() {
            // 인기 검색어 조회
            this.getPopularKeyword();
        },
        methods: {
            /**
             * 사용자가 검색 키워드를 입력할때 발생하는 이벤트
             */
            typeKeyword: function typeKeyword() {
                var self = this;
                var keyword = $("#keyword_str").val();

                if (keyword.length !== 0) {
                    //자동완성어
                    this.findAutocompleteResult(keyword);
                } else {
                    //인기검색어
                    self.showPopularKeyword = true;
                }
            },

            /**
             * 인기검색어 조회
             */
            getPopularKeyword: function getPopularKeyword() {
                var self = this;
                this.popularKeywordList = [];
                $.get(this.searchProxyURL + "/querylog/popularKeyword").done(function (data) {
                    if (data != null) {
                        self.drawPopularKeyword(data);
                    }
                });
            },

            /**
             * 인기 검색어 그리기
             */
            drawPopularKeyword: function drawPopularKeyword(popularBooks) {
                var self = this;
                popularBooks.forEach(function (item) {
                    var popularBook = {
                        bookName: item.keyword
                    };
                    self.popularKeywordList.push(popularBook);
                });
                var number = 0;

                for (var _iterator = this.popularKeywordList, _isArray = Array.isArray(_iterator), _i = 0, _iterator = _isArray ? _iterator : _iterator[Symbol.iterator]();;) {
                    var _ref;

                    if (_isArray) {
                        if (_i >= _iterator.length) break;
                        _ref = _iterator[_i++];
                    } else {
                        _i = _iterator.next();
                        if (_i.done) break;
                        _ref = _i.value;
                    }

                    var keyword = _ref;
                    number = number + 1;
                    keyword.number = number.toString().length < 2 ? "0" + number : number;

                    if (number > 5) {
                        keyword.direction = "right";
                        self.rightPopularKeywordList.push(keyword);
                    } else {
                        keyword.direction = "left";

                        if (number < 4) {
                            keyword.bold = 'active';
                        }

                        self.leftPopularKeywordList.push(keyword);
                    }
                }
            },

            /**
             * 자동완성 검색 결과
             */
            findAutocompleteResult: function findAutocompleteResult(keyword) {
                var self = this;
                var param = {
                    q: keyword,
                    types: "education,book,contents",
                    fq: keyword
                };
                $.post(this.searchProxyURL + "/autocomplete/search", param).done(function (data) {
                    if (data != null) {
                        var bookResult = data.booksResults;
                        self.changeBookList(keyword, bookResult);
                        var contentResult = data.contentsResults;
                        self.changeContentList(keyword, contentResult);
                        var educationResult = data.educationResults;
                        self.changeEducationList(keyword, educationResult);
                    }
                });
                this.showPopularKeyword = false;
            },

            /**
             * 자동완성어에서 BOOK 부분에서 마우스를 올렸을 시에 발생하는 이벤트
             * @param bookId
             */
            mouseOverBook: function mouseOverBook(bookId) {
                var currentBook = this.bookList.filter(function (book) {
                    return book.id === bookId;
                });
                this.changeBookSummary(currentBook[0]);
            },

            /**
             * 책 소개 변경
             * @param book
             */
            changeBookSummary: function changeBookSummary(book) {
                if (book !== {}) {
                    this.bookSummary.name = book.name;
                    this.bookSummary.originName = book.originName;
                    this.bookSummary.author = book.author;
                    this.bookSummary.bookType = book.bookType;
                    this.bookSummary.imgUrl = book.imgUrl;
                    this.bookSummary.link = book.link;
                    this.bookSummary.publishingCompany = book.publishingCompany;
                    this.bookSummary.date = book.date;
                    this.bookSummary.bookType = book.bookType;
                }
            },

            /**
             * BOOK 리스트 변경
             */
            changeBookList: function changeBookList(keyword, bookResultList) {
                var self = this;
                self.bookList = [];
                var index = 0;

                if (bookResultList != null) {
                    for (var _iterator2 = bookResultList, _isArray2 = Array.isArray(_iterator2), _i2 = 0, _iterator2 = _isArray2 ? _iterator2 : _iterator2[Symbol.iterator]();;) {
                        var _ref2;

                        if (_isArray2) {
                            if (_i2 >= _iterator2.length) break;
                            _ref2 = _iterator2[_i2++];
                        } else {
                            _i2 = _iterator2.next();
                            if (_i2.done) break;
                            _ref2 = _i2.value;
                        }

                        var result = _ref2;

                        if (index < 10) {
                            var book = {};
                            book.id = result.code;
                            book.originName = result.title;
                            book.name = JSON.parse(JSON.stringify(self.replaceHighlightWord(result.title, keyword)));
                            book.link = result.code;
                            book.imgUrl = self.getBookImageURL(result.code);
                            book.author = result.author;
                            book.publishingCompany = result.brand;
                            book.date = result.pubDate;
                            book.bookType = result.bookType;
                            self.bookList.push(book); //책 미리보기 변경

                            self.changeBookSummary(self.bookList[0]);
                        }
                    }
                } else {
                    self.initBookSummary();
                }
            },

            /**
             * 책 이미지 주소 가져오기
             */
            getBookImageURL: function getBookImageURL(bookCode) {
                var imgURL;

                if (bookCode.substr(0, 1) === "E") {
                    imgURL = '/data/ebook/' + bookCode + '_m.jpg';
                } else {
                    imgURL = '/data/books/' + bookCode + "_m.jpg";
                }

                return imgURL;
            },

            /**
             * 검색 결과가 없을 경우 책 미리보기 초기화
             */
            initBookSummary: function initBookSummary() {
                this.bookSummary = {
                    'imgUrl': '',
                    'link': '',
                    'name': '',
                    'author': '',
                    'publishingCompany': '',
                    'date': '',
                    'bookType': ''
                };
            },

            /**
             * education 리스트 변경
             */
            changeEducationList: function changeEducationList(keyword, educationResultList) {
                var self = this;
                self.educationList = [];
                var index = 0;

                if (educationResultList != null) {
                    for (var _iterator3 = educationResultList, _isArray3 = Array.isArray(_iterator3), _i3 = 0, _iterator3 = _isArray3 ? _iterator3 : _iterator3[Symbol.iterator]();;) {
                        var _ref3;

                        if (_isArray3) {
                            if (_i3 >= _iterator3.length) break;
                            _ref3 = _iterator3[_i3++];
                        } else {
                            _i3 = _iterator3.next();
                            if (_i3.done) break;
                            _ref3 = _i3.value;
                        }

                        var result = _ref3;

                        if (index < 3) {
                            var content = {};
                            content.title = JSON.parse(JSON.stringify(self.replaceHighlightWord(result.title, keyword)));
                            content.link = result.code;
                            self.educationList.push(content);
                            index = index + 1;
                        }
                    }
                }
            },

            /**
             * contents 리스트 변경
             */
            changeContentList: function changeContentList(keyword, contentResult) {
                var self = this;
                self.contentList = [];
                var index = 0;

                if (contentResult != null) {
                    for (var _iterator4 = contentResult, _isArray4 = Array.isArray(_iterator4), _i4 = 0, _iterator4 = _isArray4 ? _iterator4 : _iterator4[Symbol.iterator]();;) {
                        var _ref4;

                        if (_isArray4) {
                            if (_i4 >= _iterator4.length) break;
                            _ref4 = _iterator4[_i4++];
                        } else {
                            _i4 = _iterator4.next();
                            if (_i4.done) break;
                            _ref4 = _i4.value;
                        }

                        var result = _ref4;

                        if (index < 3) {
                            var content = {};
                            content.title = JSON.parse(JSON.stringify(self.replaceHighlightWord(result.title, keyword)));
                            content.link = result.code;
                            self.contentList.push(content);
                            index = index + 1;
                        }
                    }
                }
            },

            /**
             * 하이라이트 표시
             */
            replaceHighlightWord: function replaceHighlightWord(str, highlightKeyword) {
                var replaceHighlightKeyword = highlightKeyword.replace(/[^a-z0-9가-힣_ ]/gi, '');
                var regEx = new RegExp(replaceHighlightKeyword, "gi");
                str = str.replace(regEx, function (a) {
                    return '<span class="keysel">' + a + '</span>';
                });
                return str;
            },

            /**
             * 검색 버튼 클릭 시
             * @param e
             */
            clickSearch: function clickSearch(e) {
                if (!this.keyword || this.keyword.length === 0) {
                    alert("검색어를 입력하세요");
                    $("#keyword_str").focus();
                    e.preventDefault();
                } else {
                    location.href = "/search/search_list.html?keyword=" + this.keyword;
                }
            },

            /**
             * 인기 검색어 클릭시
             * @param book
             */
            clickPopularKeyword: function clickPopularKeyword(book) {
                var searchKeyword = book.bookName;
                location.href = "/search/search_list.html?keyword=" + searchKeyword;
            },

            /**
             * 책 검색어 클릭시 책 상세로 이동
             * @param book
             */
            clickBookKeyword: function clickBookKeyword(book) {
                location.href = "/store/books/look.php?p_code=" + book.link;
            },

            /**
             * 교육 클릭시 교육 상세로 이동
             * @param education
             */
            clickEducationKeyword: function clickEducationKeyword(education) {
                location.href = "/store/education/edu_view.html?p_code=" + education.link;
            },

            /**
             * 컨텐츠 클릭시 컨텐츠 상세로 이동
             * @param contents
             */
            clickContentKeyword: function clickContentKeyword(content) {
                location.href = "/channel/category/category_view.html?cms_code=" + content.link;
            }
        }
    });
</script>
<!-- //메인 검색 및 카테고리 영역 -->
<SCRIPT LANGUAGE="JavaScript">

  function Cart(strMode) {

    var m_idx = "";
    var p_code = "B2587075793";
    var opt = $("input:radio[name=pay_choice]:checked").val();
    var opt_size = $("input:radio[name=pay_choice]").size();
    var ea = 1;
    
    if(opt_size > 0 && typeof opt =="undefined") {
      alert("상품을 선택해 주세요");
      return;
    }

    if(typeof opt =="undefined") {
      opt = p_code.substring(0,1);
    }
    
    $.ajax({
      type : "post",
      url : "/store/ajax.addCart.php",
      data : { "p_code":p_code,"m_idx":m_idx,"ea":ea,"opt":opt},
      dataType : "text",
      success : function(result, status){
        if(strMode=='cart') { 
          if(result ==1) { 
            $('#cartmsg').show();
            $('#cartmsg2').hide();
            add_cart_layer_open('layer_add_cart');
          } else if(result==0) {
            $('#cartmsg2').show();
            $('#cartmsg').hide();
            add_cart_layer_open('layer_add_cart');
            return;
          }
        }  else {
          location.href="/myhanbit/cart.html";
        }
      }          
    });            
  }  

  // 견본도서 장바구니 담기. by coffin. 2017-04-12
  function Cart_p(strMode) {
    var m_idx = "";
    var p_code = "B2587075793";
    var ea = 1;

    if(typeof opt =="undefined") {
      opt = p_code.substring(0,1);
    }

    $.ajax({
      type : "post",
      url : "/academy/professor/ajax.addCart.php",
      data : { "p_code":p_code,"m_idx":m_idx,"ea":ea,"opt":opt},
      dataType : "text",
      success : function(result, status){      
        if(strMode=='cart') { 
          location.href='/academy/professor/cart.html';
        }  else {
          location.href='/academy/professor/cart.html';
        }
      }          
    });            
  }  

  function Wishlist() {

    var m_idx = "";
    var p_code = "B2587075793";
    var opt = $("input:radio[name=pay_choice]:checked").val();
    var opt_size = $("input:radio[name=pay_choice]").size();
    var ea = 1;
    var comment = "";

    if(m_idx =="") {
      alert('로그인이 필요한 서비스 입니다.');
      document.location.href='/member/login.html';
      return;
    }

    if(opt_size > 0 && typeof opt =="undefined") {
      alert("상품을 선택해 주세요");
      return;
    }
    
    $.ajax({
      type : "post",
      url : "/store/ajax.addWishlist.php",
      data : { "p_code":p_code,"m_idx":m_idx,"ea":ea,"opt":opt,"comment":comment},
      dataType : "text",
      success : function(result, status){
        if(result ==1) { 
          alert("해당 상품을 위시리스트에 담았습니다");
          return;
        } else if(result==0) {
          alert("이미 위시리스트에 추가된 상품입니다.");
          return;
        }
      }
    });            
  }  

  /*---------------------------------------------------------------
    리뷰 SCRIPT
  --------------------------------------------------------------- */  
  //리뷰 팝업요청 버튼 
  function review_start(){  
    var m_idx = '';
    var re_chk_num = '0';    
    
    if(!m_idx) { 
      alert("로그인후 리뷰 등록이 가능합니다.");
      document.location.href='/member/login.html';
      return;
    }  
      
    if(re_chk_num<1){
      review_layer_open('layer_review');
      $('#hbr_content_review').html('<textarea class="i_textarea" name="hbr_content" id="hbr_content" ></textarea>');
      return;
    }else{
      alert('리뷰등록은 도서당 한번만 가능합니다.');
      return;
    }
  }  
  
  //리뷰 등록 취소 버튼    
  function frm_reset(){    
    $('.review_layer').fadeOut();    
  }  


  //리뷰등록  처리 버튼
  function review_proc(str,idx){      
    
    document.getElementById("hbr_content").value = myeditor.outputBodyHTML();        

    if(!$.trim($("#hbr_title").val()).length) {
      alert("리뷰제목을 입력해주세요"); $("#hbr_title").focus();return;
    }    

    if(!$.trim($("#hbr_score").val()).length) {
      alert("별점 평가를 선택해주세요"); $("#hbr_score").focus();return;
    }
    
          
    if($("#hbr_content").val().length <= 13) {
      alert("리뷰내용을 입력해주세요"); $("#hbr_content").focus();return;
    }      
    
    var type_str      =  str;  
    var hbr_idx       =  idx;    
    var fk_p_code    = $("#fk_p_code").val();  
    var fk_m_idx      = $("#fk_m_idx").val();  
    var hbr_type     = $("#hbr_type").val();            
    var hbr_title      = $("#hbr_title").val();  
    var hbr_score    = $("#hbr_score").val();  
    var hbr_content   = $("#hbr_content").val();  

    var result_postdata= {  
        type_str    :   type_str,
        hbr_idx      :  hbr_idx,
        fk_p_code   :   fk_p_code,
        fk_m_idx     :   fk_m_idx,
        hbr_type     :  hbr_type,
        hbr_title    :   hbr_title,        
        hbr_score   :   hbr_score,
        hbr_content :   hbr_content    
    }                                    
    $.ajax({
      type : "post",
      url : "/store/ajax.addReview.php",
      data:result_postdata,          
      contentType: "application/x-www-form-urlencoded; charset=UTF-8",
      success : function(data){                
        if(data  >0){                              
          $('.review_layer').fadeOut();  
          //location.reload();
          var currentUrl = window.location.href.split('&');                  
          //url "&" 있으면 삭제후 파라미터 추가
          location.href = currentUrl[0]+'&tid=review';
          //location.href='/store/books/look.php?p_code=B2587075793&tid=review';                  
        }            
      }
    });
  }  

  //리뷰 수정, 삭제
  function review_frm_proc(str,idx,n){

    if(str =="redel"){
      if(!confirm(" 삭제 후 데이터는 복구되지 않습니다. \n 해당 항목을 삭제 하시겠습니까? ")){ return; }  
    }
    var result_postdata= {  str : str, idx:idx}    
    var result_url = "/inc/inc_store_layer_popup_review_ajax.php";    
                                                    
    $.ajax({
      type:"post",
      url: result_url,
      data:result_postdata,
      contentType: "application/x-www-form-urlencoded; charset=UTF-8",
      success: function(data){            

        if(str == "redel" && data ==1 ){
          alert("리뷰가 삭제되었습니다.");              
          location.reload();
          //location.href="/store/books/look.php?p_code=$p_code#review_tab";
          
        }else{
          $("#review_popup_div").html("");        
          $("#review_popup_div").html(data);        
          review_layer_open('layer_review');
        }
      }         
    });  
    
  }
      
  //리뷰 댓글 리스트  ::  리뷰 idx , div_num
  function comment_list(idx,n){      
    //alert(idx+"="+n)
    var result_postdata= {  hbr_idx : idx, div_n:n}    
    var result_url = "/store/rev_comm_list.php";    
                                                    
    $.ajax({
      type:"post",
      url: result_url,
      data:result_postdata,
      contentType: "application/x-www-form-urlencoded; charset=UTF-8",
      success: function(data){          
        $("#comment_div_"+n).html(data);        
      }         
    });          
  }    
  
  //등록, 수정, 삭제    ::  처리 str , 리뷰 div_num , 리뷰 idx , 댓글 div_num,  댓글 idx
  function comment_proc( str ,div_n, idx , div2_n ,re_idx){      
    var fk_hbr_idx     = idx;    
    var fk_m_idx     = $("#fk_m_idx").val();        
    var comment    = $("#comment_"+div_n).val();                
    var hbrc_idx     = re_idx;
    
    if(!fk_m_idx) { 
      alert("로그인후 댓글을 작성 하실수있습니다.");
      document.location.href='/member/login.html';
      return;  
    }    
    if(str == "del"){
      if(!confirm(" 삭제 후 데이터는 복구되지 않습니다. \n 해당 항목을 삭제 하시겠습니까? ")){ return; }  
    }else if(str == "ins"||str == "mod"){    
      if(!comment){
        alert("내용을  입력해주세요.");      
        $("#comment_"+div_n).focus(); return;  
      }
    }
            
    var result_postdata= {            
      str:str,            
      div_n:div_n,    
      fk_hbr_idx : fk_hbr_idx,  
      fk_m_idx : fk_m_idx,  
      comment:comment,    
      hbrc_idx:hbrc_idx,
      div2_n:div2_n
    }    
    var result_url = "/store/rev_comm_proc.php";    
                                                    
    $.ajax({
      type:"post",
      url: result_url,
      data:result_postdata,
      contentType: "application/x-www-form-urlencoded; charset=UTF-8",
      success: function(data){      
        if(str == "mod_f"){          
          $("#re_"+div_n+"_"+div2_n).html("");
          $("#re_"+div_n+"_"+div2_n).html(data);
        }else{
          //var ment = data.trim();
          alert(data.trim());  
          comment_list( fk_hbr_idx,div_n); //댓글 리스트          
        }          
      }         
    });            
  }    
  /*---------------------------------------------------------------
    리뷰 SCRIPT 
  --------------------------------------------------------------- */

  /*---------------------------------------------------------------
    오탈자 SCRIPT
  --------------------------------------------------------------- */  
  //오탈자 리스트     
  function misprint_get_list(str){    
    var p_code     = 'B2587075793';          
    var conf      = $("#confirm_type option:selected").val();        
    var p_sort_type  = $("#p_sort_type").val();  
    var r_sort_type    = $("#r_sort_type").val();  
    if(!str)str = "page";    
    
    //정렬
    if(str == "page"){    
      if( p_sort_type == "DE"){ p_sort_type = "A";
      }else if( p_sort_type == "" ||  p_sort_type == "A"){  p_sort_type = "DE";
      }      
      r_sort_type = "";
    }
    if(str == "regdt"){    
      if( r_sort_type == "DE"){ r_sort_type = "A";
      }else if(  r_sort_type == "" ||r_sort_type == "A"){  r_sort_type = "DE";
      }
      p_sort_type = "";    
    }                  
    $("#p_sort_type").val(p_sort_type);
    $("#r_sort_type").val(r_sort_type);
    //alert(p_sort_type+"=="+r_sort_type);
            
    $.ajax({
      type : "post",
      url : "/inc/inc_store_detail_misprint_ajaxList.php",
      data:{  fk_p_code:p_code , conf :conf , p_sort_type : p_sort_type , r_sort_type:r_sort_type },          
      contentType: "application/x-www-form-urlencoded; charset=UTF-8",
      success : function(data){      
        $("#misprint_con_div").html("");
        //$("#misprint_con_div").html(data);            
        $(".misprint_list").eq(0).html(data);            
        $(".misprint_list").eq(1).html(data);            
      }
    });            
  }
  
  //오탈자 팝업요청 버튼 
  function misprint_start(){    
    var m_idx = '';  
    if(!m_idx) { 
      alert("로그인 후 오탈자 등록이 가능합니다.");
      document.location.href='/member/login.html';
      return;
    }    
    misprint_layer_open('layer_misprint');return;    
  }  
  
  //오탈자 등록 취소 버튼    
  function misprint_reset(){    
    $('.misprint_layer').fadeOut();    
  }  

  //오탈자등록  처리 버튼
  function misprint_proc(str,idx){  
    var fk_m_idx        = $("#fk_m_idx").val();       
    if(!fk_m_idx) { 
      alert("로그인 후 사용이 가능합니다.");
      document.location.href='/member/login.html';
      return;
    }
    
    $("#err_info").val( myeditor_err.outputBodyHTML());            
    
    var type_str        =  str;          
    var fk_m_idx        = $("#fk_m_idx").val();       
    var fk_p_code      = $("#fk_p_code").val();          
    var err_type        = $("#err_type option:selected").val();      
    var err_p_version    = "";  
    var err_p_page      = $("#err_p_page").val();       
    var err_location    = $("#err_location").val();   
    var err_info        = $("#err_info").val();   
    var err_file        = $("#err_file").val();                                                                  
                
                    
    if($("#err_type option:selected").val() =="") {
      alert("구분을선택해주세요");$("#err_type").focus();return;
    }
    
    if($(":checkbox[name='err_p_version[]']:checked").length < 1) {
      alert("상품버전을 선택해주세요");$("#err_p_version").focus();return;
    }
          
    if(!err_p_page) {
      alert("페이지를 입력해주세요");$("#err_p_page").focus();return;
    }      
    
    if(!err_location) {
      alert("위치정보를 입력해주세요");$("#err_location").focus();return;
    }  
        
    var err_info_len = $("#err_info").val().length;    
    if(err_info_len <20) {
      alert("내용을 입력해주세요");$("#err_info").focus();return;
    }                
  
    err_p_version = "";
    $('input:checkbox[name="err_p_version[]"]').each(function() {         
      if(this.checked){
        err_p_version+=','+$(this).val();      
      }                    
     });     
    err_p_version = err_p_version.substring(1,err_p_version.length);         
            
    var result_postdata= {  
         type_str        :  type_str,
         fk_m_idx        :  fk_m_idx,        
         fk_p_code      :   fk_p_code,      
         err_type        :  err_type,        
         err_p_version    :  err_p_version,  
         err_p_page      :   err_p_page,      
         err_location      :  err_location,          
         err_info        :  err_info,    
         err_file          :  err_file              
    }                                    
    $.ajax({
      type : "post",
      url : "/store/ajax.addMisprint.php",
      data:result_postdata,          
      contentType: "application/x-www-form-urlencoded; charset=UTF-8",
      success : function(data){  
        if(data  >0){    
          $('.misprint_layer').fadeOut();      
          var currentUrl = window.location.href.split('&');        
          
          //url "&" 있으면 삭제후 파라미터 추가
          location.href = currentUrl[0]+'&tid=misprint';
          //location.href='/store/books/look.php?p_code=B2587075793&tid=misprint';                  
          //location.reload(true);
        }            
      }
    });  
  }    
  
  //오탈자 인쇄하기
  function misprint_print(){      
    var p_code       = 'B2587075793';          
    var conf        = $("#confirm_type option:selected").val();        
    var p_sort_type  = $("#p_sort_type").val();  
    var r_sort_type    = $("#r_sort_type").val();      
    
    //var sort_type;    
    //if(p_sort_type)sort_type = p_sort_type ;
    //if(r_sort_type)sort_type = r_sort_type ;
        
    var url_full = "/store/books/misprint_view.html?fk_p_code="+p_code+"&conf="+conf+"&p_sort_type="+p_sort_type+"&r_sort_type="+r_sort_type;      
    
    var openNewWindow = window.open("about:blank");
        openNewWindow.location.href = url_full;                
  }
              
  /*---------------------------------------------------------------
    오탈자 SCRIPT 
  --------------------------------------------------------------- */

  /*---------------------------------------------------------------
    도서인증 SCRIPT
  --------------------------------------------------------------- */  
  function book_auth(){  
    var fk_m_idx        = $("#fk_m_idx").val();       
    if(!fk_m_idx) { 
      alert("로그인 후 사용이 가능합니다.");
      document.location.href='/member/login.html';
      return;
    }
          
    var fk_m_idx      = $("#fk_m_idx").val();       
    var fk_p_code      = $("#fk_p_code").val();          
    var buy_date      = $("#buy_date").val();      
    var buy_site        = $("#buy_site").val();      
    var add_code      = $("#add_code").val();                                                                                
          
    if(!buy_site) {
      alert("구입처를 선택해주세요");$("#buy_site").focus();return;
    }  

    if(!buy_date) {
      alert("구입일을 입력해주세요");$("#buy_date").focus();return;
    }          
    
    if(!add_code) {
      alert("부가기호를 입력해주세요");$("#add_code").focus();return;
    }  
    
    $("#cfrm").submit();
    
    /*    
    var result_postdata= {  
         fk_m_idx      :  fk_m_idx,        
         fk_p_code      :   fk_p_code,      
         buy_date      :  buy_date,        
         buy_site        :  buy_site,  
         add_code      :   add_code,          
    }                                    
    $.ajax({
      type : "post",
      url : "/store/ajax.addBookauth.php",
      data:result_postdata,          
      contentType: "application/x-www-form-urlencoded; charset=UTF-8",
      success : function(data){          
        if(data  >0){                        
          $('.certification_layer').fadeOut();      
          location.reload(true);
        }            
      }
    });  
    */
  }
  /*---------------------------------------------------------------
    도서인증 SCRIPT
  --------------------------------------------------------------- */  

</SCRIPT>

