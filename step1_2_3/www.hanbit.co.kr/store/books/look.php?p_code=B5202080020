  function Cart(strMode) {

    var m_idx = "";
    var p_code = "B5202080020";
    var opt = $("input:radio[name=pay_choice]:checked").val();
    var opt_size = $("input:radio[name=pay_choice]").size();
    var ea = 1;
    
    if(opt_size > 0 && typeof opt =="undefined") {
      alert("상품을 선택해 주세요");
      return;
    }

    if(typeof opt =="undefined") {
      opt = p_code.substring(0,1);
    }
    
    $.ajax({
      type : "post",
      url : "/store/ajax.addCart.php",
      data : { "p_code":p_code,"m_idx":m_idx,"ea":ea,"opt":opt},
      dataType : "text",
      success : function(result, status){
        if(strMode=='cart') { 
          if(result ==1) { 
            $('#cartmsg').show();
            $('#cartmsg2').hide();
            add_cart_layer_open('layer_add_cart');
          } else if(result==0) {
            $('#cartmsg2').show();
            $('#cartmsg').hide();
            add_cart_layer_open('layer_add_cart');
            return;
          }
        }  else {
          location.href="/myhanbit/cart.html";
        }
      }          
    });            
  }  

  // 견본도서 장바구니 담기. by coffin. 2017-04-12
  function Cart_p(strMode) {
    var m_idx = "";
    var p_code = "B5202080020";
    var ea = 1;

    if(typeof opt =="undefined") {
      opt = p_code.substring(0,1);
    }

    $.ajax({
      type : "post",
      url : "/academy/professor/ajax.addCart.php",
      data : { "p_code":p_code,"m_idx":m_idx,"ea":ea,"opt":opt},
      dataType : "text",
      success : function(result, status){      
        if(strMode=='cart') { 
          location.href='/academy/professor/cart.html';
        }  else {
          location.href='/academy/professor/cart.html';
        }
      }          
    });            
  }  

  function Wishlist() {

    var m_idx = "";
    var p_code = "B5202080020";
    var opt = $("input:radio[name=pay_choice]:checked").val();
    var opt_size = $("input:radio[name=pay_choice]").size();
    var ea = 1;
    var comment = "";

    if(m_idx =="") {
      alert('로그인이 필요한 서비스 입니다.');
      document.location.href='/member/login.html';
      return;
    }

    if(opt_size > 0 && typeof opt =="undefined") {
      alert("상품을 선택해 주세요");
      return;
    }
    
    $.ajax({
      type : "post",
      url : "/store/ajax.addWishlist.php",
      data : { "p_code":p_code,"m_idx":m_idx,"ea":ea,"opt":opt,"comment":comment},
      dataType : "text",
      success : function(result, status){
        if(result ==1) { 
          alert("해당 상품을 위시리스트에 담았습니다");
          return;
        } else if(result==0) {
          alert("이미 위시리스트에 추가된 상품입니다.");
          return;
        }
      }
    });            
  }  

