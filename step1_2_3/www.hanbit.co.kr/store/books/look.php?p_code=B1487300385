                                    <input type="hidden" id="fk_hbr_idx" name="fk_hbr_idx" value="6251">   
                  <li class="article hide">
                    <div class="review_li">
                      <a class="trigger" href="javascript:;">
                        <div class="info"><strong>sp***</strong><span>l</span>2019-01-21</div>
                        <script>
                          $(function() {    
                            $('.star_rate'+3).raty({
                              readOnly: true,
                              score: 4,
                              starHalf   : '/images/common/star_s-half.png',
                              starOff    : '/images/common/star_s-off.png',
                              starOn     : '/images/common/star_s-on.png'
                            });
                            
