                                    <input type="hidden" id="fk_hbr_idx" name="fk_hbr_idx" value="5945">   
                  <li class="article hide">
                    <div class="review_li">
                      <a class="trigger" href="javascript:;">
                        <div class="info"><strong>agis***</strong><span>l</span>2018-10-13</div>
                        <script>
                          $(function() {    
                            $('.star_rate'+7).raty({
                              readOnly: true,
                              score: 5,
                              starHalf   : '/images/common/star_s-half.png',
