          </ul>
        </div>

        <div class="sugg_cont" v-if="contentList.length !== 0">
          <h3 class="tit">CONTENTS</h3>
          <ul class="cont_list">
            <li v-for="content in contentList">&#149; <a @click="clickContentKeyword(content)"><span v-html="content.title"></span></a></li>
          </ul>
        </div>
      </div>
    </div>
    <!-- //② 검색결과/검색서제스쳔 -->

