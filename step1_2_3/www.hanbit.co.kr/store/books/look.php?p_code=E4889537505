              <a :class="keyword.bold" @click="clickPopularKeyword(keyword)">
                  {{ keyword.number }} {{ keyword.bookName }}
              </a>
            </li>
          </ul>
          <ul class="right">
            <li v-for="keyword in rightPopularKeywordList">
              <a :class="keyword.bold" @click="clickPopularKeyword(keyword)">
                  {{ keyword.number }} {{ keyword.bookName }}
              </a>
            </li>
          </ul>
        </div>
