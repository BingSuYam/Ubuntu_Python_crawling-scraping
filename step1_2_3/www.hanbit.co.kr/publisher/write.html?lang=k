<script>
	function book_apply() { 

		var agreeVal = $(':radio[name="term_agree"]:checked').val();



		if(!$.trim($("#m_name").val()).length) {
			alert("이름을 입력해주세요.");
			$("#m_name").focus();
			return;
		}

		if(!$.trim($("#m_email_1").val()).length) {
			alert("이메일을 입력해주세요.");
			$("#m_email_1").focus();
			return;
		}

		if(!$.trim($("#m_email_2").val()).length) {
			alert("이메일을 입력해주세요.");
			$("#m_email_2").focus();
			return;
		}

		if(!$.trim($("#m_mobile_1").val()).length || !$.trim($("#m_mobile_2").val()).length || !$.trim($("#m_mobile_3").val()).length) {
			alert("연락처를 입력해주세요.");
			return;
		}

		if(!$.trim($("#p_brand").val()).length) {
			alert("브랜드를 선택해주세요.");
			$("#p_brand").focus();
			return;
		}

		if(!$.trim($("#book_genre").val()).length) {
			alert("분야를 선택해주세요.");
			$("#book_genre").focus();
			return;
		}

		if(!$.trim($("#book_title").val()).length) {
			alert("책제목을 입력해주세요.");
			$("#book_title").focus();
			return;
		}

		if(!$.trim($("#book_cont").val()).length) {
			alert("원고내용을 입력해주세요.");
			$("#book_cont").focus();
			return;
		}

		if(!$.trim($("#book_file1").val()).length) {
			alert("기획서를 등록해주세요");
			$("#book_file1").focus();
			return;
		}
		/*
		if(!$.trim($("#book_file2").val()).length) {
			alert("원고를 첨부해주세요");
			$("#book_file2").focus();
			return;
		}
		*/

		if(agreeVal !="Y") {
			alert("개인정보 취급 및 동의 처리하셔야 합니다.");
			return;
		}
		
		$("#frm").attr("action","./indb.write.php");
		$("#frm").submit();
	}
</script>
