    <!-- 홈페이지 댓글 모듈 영역 -->
          <div class="detail_reply_area">
        <!-- 댓글 입력 -->
        <form name="re_frm" id="re_frm" method="post">                      
          <input type="hidden" id="writer"   name="writer" value="">
          <input type="hidden" id="fk_m_idx"  name="fk_m_idx" value="">          
          <input type="hidden" id="fk_bd_id"  name="fk_bd_id" value="">
          <input type="hidden" id="hbe_idx"  name="hbe_idx" value="130">          
          
          <div class="reply_event_form">
                      <fieldset>
                      <legend>댓글 입력</legend>
                          <label><textarea name="comment" id="comment" class="i_textarea"  placeholder="로그인 후 이용해 주시기 바랍니다."></textarea></label>                          
                          <label><button  type="button" name="" value="작성" class="btn_repl" onclick="event_reply_data('ins','');">댓글 작성</button></label>
                      </fieldset>
          </div>          
        </form>
        <!--// 댓글 입력 -->
        
        <!-- 댓글 리스트 -->
        <div  id="event_reply_div"></div>
        <!--// 댓글 리스트 -->
      </div>
      <script>  
              //리플 폼 노출
              function event_reply_frm(str,re_idx,re_com_idx,status){                                 
                var result_postdata= { 
                  str:str,
                  re_idx : re_idx   ,
                  re_com_idx:re_com_idx 
                }              
                                                          
          $.ajax({
            type:"post",          
            url: "/event/event_reply_frm.php",
            data:result_postdata,
            contentType: "application/x-www-form-urlencoded; charset=UTF-8",
            success: function(data){                  
              if(str == "mod"){
                $("#mod_com_div_"+re_idx).html();                                            
                $("#mod_com_div_"+re_idx).html(data);                      
              }                                
              if(str == "re_mod"){
                $("#mod_recom_div_"+re_com_idx).html();                            
                $("#mod_recom_div_"+re_com_idx).html(data);                      
              }              
            }         
                });                                                                 
              }              
                      
              //댓글달기       
              // @str:상태값  @idx: 댓글idx
              function event_reply_data(str,idx){                                                               
          var fk_hbe_idx   = $("#hbe_idx").val();                                    
          var fk_bd_id     = $("#fk_bd_id").val();                
          var fk_m_idx     = $("#fk_m_idx").val();        
          var writer       = $("#writer").val();                                      
          
          var comment      = $("#comment");    
          var re_comment    = $("#re_comment_"+idx);
          var mod_comment      = $("#mod_comment");
          var mod_re_comment    = $("#mod_re_comment_"+idx);
          
          if(str=="ins"){ comment = comment;
          }else if(str=="re_ins"){comment = re_comment;
          }else if(str=="mod"){comment = mod_comment;
          }else if(str=="re_mod"){comment = mod_re_comment;
          }
                      
          if(!fk_m_idx){alert("로그인후 댓글을 작성하실수있습니다.");return;  }                
          if(str == "ins"||str == "mod"||str == "re_mod"||str == "re_ins"){    
            if(!comment.val()){
              alert("내용을  입력해주세요.");comment.focus(); return;  
            }                          
          }else if(str == "del" || str == "re_del"){
            if(!confirm(" 삭제 후 데이터는 복구되지 않습니다. \n 해당 항목을 삭제 하시겠습니까? ")){ return; }  
            
          }            
          var comment_val = comment.val();                    
          var result_postdata= { str:str, fk_m_idx : fk_m_idx, writer : writer,  fk_hbe_idx : fk_hbe_idx,  fk_bd_id:fk_bd_id,
            comment:comment_val ,idx:idx
          }              
                                                          
          $.ajax({
            type:"post",
            url: "/event/event_reply_proc.php",
            data:result_postdata,
            contentType: "application/x-www-form-urlencoded; charset=UTF-8",
            success: function(data){                  
              alert($.trim(data));  
              event_reply_list('130');                    
            }         
                });                  
              }                  
          
        //리뷰 댓글 리스트  ::  리뷰 idx , div_num
        function event_reply_list(fk_hbe_idx){                                                                                        
          $.ajax({
            type:"post",
            url: "/event/event_reply_list.php",
            data: {  fk_hbe_idx : fk_hbe_idx , reply_page : "0"},
            contentType: "application/x-www-form-urlencoded; charset=UTF-8",
            success: function(data){            
              $("#comment").val('');          
              $("#event_reply_div").html(data);        
            }         
          });          
        }
        
        //답글 처리 버튼 
        function recom_frm(str,num){                    
          if(str=="show"){                          
            $("#recom_div_"+num).show();                            
            $("#re_btn_"+num).html("");      
            $("#re_btn_"+num).html("<a href=\"javascript:recom_frm('hide','"+num+"');\">답글취소</a>");      
            
          }else if(str=="hide"){
            $("#recom_div_"+num).hide();                                                
            $("#re_btn_"+num).html("");      
            $("#re_btn_"+num).html("<a href=\"javascript:recom_frm('show','"+num+"');\">답글</a>");      
          }            
        }    
        
        //리플리스트 VIEW              
        event_reply_list('130');        
      </script>  
      
